<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC 
	"-//SPRING//DTD BEAN//EN" 
	"http://www.springframework.org/dtd/spring-beans.dtd">
	
<beans>
	<bean id="bandaCRUDAction" class="com.bandManager.action.banda.BandaCRUDAction">
		<property name="bandaFacade" ref="bandaFacade" />
		<property name="paisFacade" ref="paisFacade" />
    </bean>
    
    <bean id="lancamentoCRUDAction" class="com.bandManager.action.lancamento.LancamentoCRUDAction">
		<property name="lancamentoFacade" ref="lancamentoFacade" />
    </bean>
    
    <bean id="musicaCRUDAction" class="com.bandManager.action.musica.MusicaCRUDAction">
		<property name="musicaFacade" ref="musicaFacade" />
    </bean>
    
    <bean id="bandaUploadArquivosAction" class="com.bandManager.action.banda.BandaUploadArquivosAction">
		<property name="bandaFacade" ref="bandaFacade" />
    </bean>
    
    <bean id="lancamentoUploadArquivosAction" class="com.bandManager.action.lancamento.LancamentoUploadArquivosAction">
		<property name="lancamentoFacade" ref="lancamentoFacade" />
    </bean>
    
    <bean id="musicaUploadArquivosAction" class="com.bandManager.action.musica.MusicaUploadArquivosAction">
		<property name="musicaFacade" ref="musicaFacade" />
    </bean>
    
    <bean id="showCRUDAction" class="com.bandManager.action.show.ShowCRUDAction">
		<property name="paisFacade" ref="paisFacade" />
		<property name="showFacade" ref="showFacade" />
    </bean>
    
    <bean id="usuarioCRUDAction" class="com.bandManager.action.usuario.UsuarioCRUDAction">
		<property name="paisFacade" ref="paisFacade" />
		<property name="usuarioFacade" ref="usuarioFacade" />
    </bean>
</beans>

